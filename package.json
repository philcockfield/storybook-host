{
  "name": "storybook-host",
  "version": "1.1.0",
  "description": "A React Storybook decorator with helpful display options for hosting components under test",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "ui": "start-storybook -p 3000 -c ./.storybook -s ./assets",
    "start": "npm run build && npm run ui",
    "test": "./node_modules/mocha/bin/mocha --require ts-node/register --watch-extensions ts,tsx 'src/**/*.test.ts{,x}'",
    "tdd": "npm run test -- --reporter min --watch",
    "lint": "node ./node_modules/tslint/bin/tslint 'src/**/*.ts{,x}' --format verbose",
    "build": "rm -rf ./lib && node ./node_modules/typescript/bin/tsc",
    "prepublish": "npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^3.0.0",
    "@types/tinycolor2": "^1.1.1",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "mobx-react-devtools": "^4.2.13",
    "radium": "^0.19.1",
    "ramda": "^0.23.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.0.0",
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.21",
    "@types/radium": "^0.18.21",
    "@types/ramda": "0.0.9",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "chai": "^3.5.0",
    "mocha": "^3.4.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.2"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/storybook-host"
  },
  "keywords": [
    "react",
    "react-storybook",
    "helper",
    "ui"
  ],
  "author": {
    "name": "Phil Cockfield",
    "email": "phil@cockfield.net",
    "url": "https://github.com/philcockfield/modules"
  },
  "homepage": "",
  "license": "MIT"
}
